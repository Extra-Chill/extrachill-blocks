{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "extrachill-blocks/ai-adventure",
	"version": "0.1.0",
	"title": "AI Adventure",
	"category": "widgets",
	"icon": "games",
	"description": "The main container block for an AI-powered text adventure.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string"
		},
		"adventurePrompt": {
			"type": "string"
		},
		"gameMasterPersona": {
			"type": "string",
			"default": "You are a helpful and creative text-based adventure game master."
		},
		"innerBlocksJSON": {
			"type": "string",
			"default": "[]"
		}
	},
	"textdomain": "extrachill-blocks",
	"editorScript": "file:../../build/ai-adventure/index.js",
	"viewScript": "file:../../build/ai-adventure/view.js",
	"editorStyle": "file:../../build/ai-adventure/editor.css",
	"providesContext": {
		"extrachill-blocks/adventurePrompt": "adventurePrompt"
	}
}